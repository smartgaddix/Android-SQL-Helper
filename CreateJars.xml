<?xml version="1.0" encoding="UTF-8"?>
<project basedir="." default="package" name="AndroidSQLHelper">

	<property name="bin" value="bin" />
	<property name="src" value="src" />

	<property name="binfile" value="bin/androidsqlhelper.jar" />

	<target name="package">
		<jar destfile="${binfile}">
			<zipfileset dir="${bin}">
				<include name="META-INF/**" />
				<include name="**/*.class" />
				<exclude name="**/R.class" />
				<exclude name="**/R$*.class" />
			</zipfileset>
		</jar>
	</target>
</project>